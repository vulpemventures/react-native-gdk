{
  "name": "@vulpemventures/react-native-gdk",
  "version": "0.0.1",
  "description": "react native turbomodule cpp for gdk",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "lib",
    "android",
    "ios",
    "cpp",
    "libgreenaddress_full.xcframework",
    "libgreenaddress-android",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-gdk-example",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "prepack": "yarn ios:gdk_prepare && yarn android:gdk_prepare",
    "ios:gdk_prepare": "./npm_scripts/ios_gdk.sh",
    "android:gdk_prepare": "./npm_scripts/android_gdk.sh",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulpemventures/react-native-gdk/react-native-gdk.git"
  },
  "author": "mattiaferrari <mattia.ferrari@soluzionifutura.it> (https://github.com/vulpemventures/react-native-gdk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vulpemventures/react-native-gdk/react-native-gdk/issues"
  },
  "homepage": "https://github.com/vulpemventures/react-native-gdk/react-native-gdk#readme",
  "publishConfig": {
    "@vulpemventures:registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.72.2",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.2",
    "@types/react": "~17.0.21",
    "@types/react-native": "0.70.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "^28.1.1",
    "pod-install": "^0.1.0",
    "prettier": "^3.2.4",
    "react": "18.2.0",
    "react-native": "0.73.3",
    "react-native-builder-bob": "^0.20.0",
    "release-it": "^15.0.0",
    "turbo": "^1.10.7",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "engines": {
    "node": ">= 18.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
